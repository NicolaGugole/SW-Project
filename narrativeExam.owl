#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( dctypes:Location
                dctypes:Text
                :BookShop
                :Event
                :Narrative
                :Partecipant
                :Publisher
                foaf:Person
              )
] .


